{
    "version": 2,
    "builds": [
      {
        "src": "./main.jsx",
        "use": "@vercel/node"
      },
      {
        "src": "public/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/src/components/pages/Home.jsx"
      }
    ]
  }
  